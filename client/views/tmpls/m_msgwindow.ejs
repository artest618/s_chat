<div class="chat_view_sub" id="<%=parseInt(chat.id)%>">
    <div class="container-fluid border-bot header bgf" style="position: fixed;width: 100%;z-index: 99999;">
        <div class="row">
            <div class="col-xs-2 col-sm-1">
                <em class="arrow get_list"></em>
            </div>
            <div class="col-xs-8 col-sm-10 mt15">
                <h3 class="f30 align-center"><%=chat.to_cname%></h3>
            </div>
            <div class="col-xs-2 col-sm-1">
                <% if (chat.to_type==3){%>
                    <span class="add" ></span>
                <%}%>

            </div>
        </div>
    </div>
    <div class="container">
        <div class="row mt100">
            <div class="col-xs-12 col-sm-12">
                <div class="row row-in helght140 bgf pro_info">
                    <div class="col-xs-4 col-sm-2 mt15">
                        <img src="../images/ads.jpg" alt="" style="width: 150px;height: 110px;" class="pro_img">
                    </div>
                    <div class="col-xs-8 col-sm-10 mt15">
                        <div class="pro_content">

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="align-center mt10">
            <a href="javascript:void(0);" class="f14 moremsgbtn">查看更多</a>
        </div>
        <div class="c_msg_list">

        </div>

    </div>
    <div class="container-fluid footer">
        <div class="col-xs-3 col-sm-2">
            <img src="../images/emoji/ssmile.png"  class="chemoji" />
            <ul class="emojipanel"></ul>
        </div>
        <div class="col-xs-6 col-sm-8">
            <input type="text" class="f26 inputmsg" placeholder="">
        </div>
        <div class="col-xs-3 col-sm-2">
            <button class="fbtnsend cf f26">发送</button>
        </div>
    </div>
    <style type="text/css">
        .footer .chemoji{
            width:60px;height:60px;position: absolute;top: 1.5rem;z-index: 1;
        }
        .emojipanel.show{
            width: 100%;height: 260px;position: absolute;z-index: 2;top: 8rem;left: -2rem;
        }
        .emojipanel.show li{
            list-style: none;
            width:280px; float:left
        }
        .emojipanel.show li img{
            width: 60px;
            height: 60px;
        }
        .footer.h_new{
            height: 350px;
        }
        a .moremsgbtn{
            text-decoration: none;
        }
    </style>
</div>