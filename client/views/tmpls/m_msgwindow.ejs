<div class="chat_view_sub" id="<%=parseInt(chat.id)%>">
    <div  id="history_header"  class="container-fluid border-bot header bgf" style="position: fixed;width: 100%;z-index: 99999;">
        <div class="row">
            <div class="col-xs-2 col-sm-1">
                <em class="arrow get_list"></em>
            </div>
            <div class="col-xs-8 col-sm-10 mt15">
                <h3 class="f30 align-center"><%=chat.to_cname%></h3>
            </div>
            <div class="col-xs-2 col-sm-1">
                <% if (chat.to_type==3){%>
                    <span class="add" ></span>
                <%}%>

            </div>
        </div>
    </div>
    <div  id="message_to_header_toapp"  class="container-fluid border-bot header bgf" style="position: fixed;width: 100%;z-index: 99999;display: none;">
        <div class="row">
            <a href="http://message_to_header_toapp">
                <div class="col-xs-2 col-sm-1">
                    <em class="arrow get_list"></em>
                </div>
            </a>
            <div class="col-xs-8 col-sm-10 mt15">
                <h3 class="f30 align-center"><%=chat.to_cname%></h3>
            </div>
            <div class="col-xs-2 col-sm-1">
                <% if (chat.to_type==3){%>
                <span class="add" ></span>
                <%}%>

            </div>
        </div>
    </div>
    <div id="message_to_header"  class="container-fluid border-bot header bgf" style="position: fixed;width: 100%;z-index: 99999;display: none;">
        <div class="row">
            <div id ="message_to_header_href" class="col-xs-2 col-sm-1">
                <em class="arrow"></em>
            </div>
            <div class="col-xs-8 col-sm-10 mt15">
                <h3 class="f30 align-center"><%=chat.to_cname%></h3>
            </div>
            <div class="col-xs-2 col-sm-1">
                <% if (chat.to_type==3){%>
                <span class="add" ></span>
                <%}%>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row mt100">
            <div class="col-xs-12 col-sm-12">
                <div class="row row-in helght140 bgf pro_info">
                    <div class="col-xs-4 col-sm-2 mt15">
                        <img src="../images/ads.jpg" alt="" style="width: 150px;height: 110px;" class="pro_img">
                    </div>
                    <div class="col-xs-8 col-sm-10 mt15">
                        <div class="pro_content">

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="align-center mt10">
            <a href="javascript:void(0);" class="f14 moremsgbtn" style="text-decoration:none;">查看更多对话</a>
        </div>
        <div class="c_msg_list">

        </div>

    </div>
    <div class="container-fluid footer">
        <div class="col-xs-3 col-sm-2 emoji_warp">
            <img src="../images/emoji/ssmile.png"  class="chemoji" />
            <!--<img src="../images/file.png" style="" class="upfilebtn" />-->
            <a href="javascript:;" class="file_warp" style="display: none;">
                <input style="" class="upfilebtn" type="file" name="file" value="">
            </a>
            <ul class="emojipanel"></ul>
        </div>

        <div class="col-xs-6 col-sm-8" style="padding:0">
            <input type="text" class="f26 inputmsg" placeholder="">
        </div>
        <div class="col-xs-3 col-sm-2">
            <button class="fbtnsend cf f26">发送</button>
        </div>
    </div>
    <style type="text/css">
        .col-in{
            width: auto;
        }
        .footer{
            z-index: 3;padding: 0 10px;
        }
        .footer .emoji_warp{
            padding: 0;
        }
        .footer .chemoji{
            width: 3rem;height: 3rem;margin-left: -.8rem;
            margin-top: .6rem;  vertical-align: top;
        }
        .emojipanel.show{
            width: 100%;height: 16.25rem;position: absolute;z-index: 2;top: 5rem;left: -4rem;
        }
        .emojipanel.show li{
            list-style: none;
            width:13.375rem; float:left
        }
        .emojipanel.show li img{
            width: 3rem;height: 3rem;
        }
        .footer.h_new{
            height: 18rem;
        }
        a .moremsgbtn{
            text-decoration: none;
        }
        .footer a.file_warp{
            display:inline-block;
            width: 3rem;
            height: 3rem;
            position:relative;
            overflow:hidden;
            margin-left: .25rem;
            margin-top: .6rem;
            background-image: url('../images/file.png');
            background-repeat: no-repeat;
            background-size: 3rem 3rem;
            border: none;
            background-color: transparent;
        }

        .footer input.upfilebtn{
            position:absolute; right:0; top:0; font-size: 3rem; opacity:0; filter:alpha(opacity=0);
        }
       /* .footer .upfilebtn{
            width: 3rem;
            height: 3rem;
            margin-left: .25rem;
            margin-top: .6rem;
            background-image: url('../images/file.png');
            background-repeat: no-repeat;
            background-size: 3rem 3rem;
            border: none;
            background-color: transparent;
            display: inline-block;
        }*/
    </style>
</div>